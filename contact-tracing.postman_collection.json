									"  type: 'object',",
									"  additionalProperties: false,",
									"  required: [",
									"    'code', 'error', 'expiresAt', 'expiresAtTimestamp', 'smsSent'",
									"  ],",
									"  properties: {",
									"    code: {",
									"      type: 'string'",
									"    },",
									"    error: {",
									"      type: 'string'",
									"    },",
									"    expiresAt: {",
